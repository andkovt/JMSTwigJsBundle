<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="twig_js.filters.trans_compiler.class">JMS\TwigJsBundle\TwigJs\Compiler\TransFilterCompiler</parameter>
    </parameters>

    <services>
        <service class="JMS\TwigJsBundle\Command\CompileTwigCommand" id="jms_twig_js.command.compile_twig_command">
            <tag name="console.command"/>
            <argument>jms:twig-js:compile</argument>
            <argument id="twig_js.compile_request_handler" type="service"/>
        </service>
    </services>
</container>
