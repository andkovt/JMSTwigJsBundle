<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="twig_js.compiler" class="TwigJs\JsCompiler" public="false">
            <argument type="service" id="twig" />
        </service>
    
        <service id="twig_js.compile_request_handler" class="TwigJs\CompileRequestHandler">
            <argument type="service" id="twig" />
            <argument type="service" id="twig_js.compiler" />
        </service>
        
        <service id="twig_js.twig_js_extension" class="TwigJs\Twig\TwigJsExtension" public="false">
            <tag name="twig.extension" alias="twig_js" />
        </service>
    </services>
</container>
